		if(typeof scrollingBanners == 'function') {
			scrollingBanners();
		}

        if (typeof bannerCallback == 'object') {
			for (i in bannerCallback) {
				if (typeof bannerCallback[i] == 'function') {
                    bannerCallback[i]();
				}
			}
		}
	}

	function side_banners_loaded()
	{
		if (typeof gateway_type == 'undefined' && typeof(banner_sides) != 'undefined' && window_min_width) {
   			BASE = 'http://orai.15min.lt/';
			jQuery.getScript('//s2.15cdn.lt/js/banners/banner_sides.js?c=?js=2');
		}
	}

	function reloadBanners(pos, wrap)
	{
		if (!bn_string) {
			return false;
		}

		bn_string = bn_string + '&rnd=' + new Date().getTime();

		jQuery(function(){
			adsplius.url = 'http://adsplius.lt/banners_15min';
			adsplius.params(bn_string);
			adsplius.call_custom(pos, wrap);
		});
	}

	jQuery(function(){
		adsplius.url = 'http://adsplius.lt/banners_15min';
		adsplius.params('p=10001&lng=lt&vt=2&cb=banners_loaded&rnd=1364259607');
		adsplius.call();
	});

