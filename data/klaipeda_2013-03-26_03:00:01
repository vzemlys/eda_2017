									.appendTo(ul);
								};
							});
	
							function focusBlurInput(el, val)
							{
								if (!el || !val) {
									return false;
								}
	
								var elm = jQuery(el);
								var el_val = elm.val();
	
								if (el_val == val) {
									elm.val('');
								} else if (!el_val) {
									elm.val(val);
								}
							}
