						/* <![CDATA[ */

						jQuery.support.placeholder = (function(){
						    var i = document.createElement('input');
						    return 'placeholder' in i;
						})();

						if (!jQuery.support.placeholder && jQuery('#city_search').val() == '') {
							jQuery('#city_search').val('Orų prognozių paieška...');

							jQuery('#city_search').bind('focus blur', function(){
								focusBlurInput(jQuery(this),'Orų prognozių paieška...');
							});
						}

						jQuery(function() {
							var cities = ["1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","1ilnius","0ilnius","1ilnius","1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","1ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius","0ilnius"];

							jQuery('#city_search').autocomplete({
								minLength: 2,
								source: function(request, response) {
									var results = $.ui.autocomplete.filter(cities, request.term);
									response(results.slice(0, 5));
							    },
								select: function(event, ui) {
									window.location = 'lt/prognozes/' + ui.item.slug;
								}
							}).data('autocomplete')._renderItem = function(ul, item) {
								return jQuery('<li></li>')
								.data('item.autocomplete', item)
								.append('<a><span class="city_name">' + item.label + '<br/><small>Ilgalaikė prognozė</small></span><span class="weather_icons weather_icon_' + item.icon_var + '"></span><span class="temperature '+ (parseInt(item.temperature) >= 0 ? 'positive' : 'negative') +'">' + item.temperature + '°C</span><div class="clear"></div></a>')
								.appendTo(ul);
							};
						});

						function focusBlurInput(el, val)
						{
							if (!el || !val) {
								return false;
							}

							var elm = jQuery(el);
							var el_val = elm.val();

							if (el_val == val) {
								elm.val('');
							} else if (!el_val) {
								elm.val(val);
							}
						}
