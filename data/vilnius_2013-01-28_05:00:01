	var bannerCallback = [];
	var gatewayCallback;

	function banners_loaded()
	{
		if (typeof gateway_type != 'undefined' || typeof gateway_fixed != 'undefined') {
			img_src = $('a img', jq_lb_10).attr('src');
			a_href = $('a', jq_lb_10).attr('href');

			if (typeof img_src != 'undefined' && typeof a_href != 'undefined') {
				function penkiolika_gtw() {
					$.getScript('//s2.15cdn.lt/js/banners/gateway_main.js?js=2', function() {
						left_jq_clk.show();
						right_jq_clk.show();

                        if (typeof gatewayCallback == 'function') {
                            gatewayCallback();
                        }
					});
				}

				if ($.browser.msie == true || $.browser.opera == true) {
					$(function() {
						penkiolika_gtw();
					});
				} else {
					penkiolika_gtw();
				}
				gates_on = true;
			} else {
				jq_lb_10.hide();
			}

			gates_on = true;
