<!-- (C) 2000-2012 Gemius SA - gemiusAudience / m_15min_orai --><script type="text/javascript">/* <![CDATA[ */ var pp_gemius_identifier = 'cntAfcchxzCiLgBi8XvGi4aATO4pYwQyk0hTiUB1Ebr.y7'; (function(d,t) {var ex; try {var gt=d.createElement(t),s=d.getElementsByTagName(t)[0],l='http'+((location.protocol=='https:')?'s':''); gt.async='true'; gt.src=l+'://lt.hit.gemius.pl/xlgemius.js'; s.parentNode.insertBefore(gt,s);} catch (ex) {}}(document,'script')); /*]]> */</script><script type="text/javascript" charset="utf-8">
/* <![CDATA[ */
    function refreshPage() {
        var link = window.location.href;
        window.location.href = link;
    }

    function reset15min() {
        setCookie('m-lst',"",-1);
        setCookie('m-qlt',"",-1);
        setCookie('m-lmt',"",-1);
        setCookie('m-itm_v3',"",-1);
        refreshPage();
    }


	function showBox(b_name, b_button)
	{
		var e = document.getElementById(b_name);
		var a1 = document.getElementById('m-settings');
		var a2 = document.getElementById('cat-lists');

		if (e.style.display !='none') {
			e.style.display = 'none';
			document.getElementById(b_button).className = document.getElementById(b_button).className.replace( /(?:^|\s)active(?!\S)/g , '' );
		} else {
			e.style.display = 'block';
			document.getElementById(b_button).className += " active";
			if (b_name != 'm-settings' && a1.style.display != 'none' ) {
				a1.style.display = 'none';
				document.getElementById('button-settings').className = document.getElementById('button-settings').className.replace( /(?:^|\s)active(?!\S)/g , '' );
			}

			if (b_name != 'cat-lists' && a2.style.display != 'none' ) {
				a2.style.display = 'none';
			}
		}
	}


	function saveItem(type, val)
	{
	    var link = window.location.href;
   		if ( type == 'm-lmt' ) {
   			link = link.replace(/\?psl\=\d{1,}/g, '');
 		}
		setCookie(type,val,365);
        window.location.href = link;
	}


    function changeList(val)
	{
        var content = '';
        for (i=1;i<=val;i++) {
           item = 'm2-item-' +i;
           elm = document.getElementById(item)
           var selected = elm.options[elm.selectedIndex].value;
           content += selected + ';';
        }

        setCookie('m-itm_v3',content,365);
        refreshPage();
	}
/* ]]> */
</script>
